
generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Leads {
  id Int @id @default(autoincrement())
  first_name String
  last_name String
  company String
  email String
  phone_number String
  assigned_to String
  source String
  gst_No String @unique
  product String
  address String
}